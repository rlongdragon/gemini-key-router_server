{
  "dependencies": {
    "@google/genai": "^1.12.0",
    "@google/generative-ai": "^0.24.1",
    "better-sqlite3": "^12.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "scripts": {
    "start": "node dist/server/index.js",
    "build": "npm run build:server && npm run build:webpanel",
    "build:server": "tsc -p src/server/tsconfig.json && npm run copy-assets",
    "copy-assets": "copyfiles -u 2 src/server/**/*.json src/server/**/*.sql dist/server",
    "build:webpanel": "vite build src/webpanel --outDir dist/webpanel --config src/webpanel/vite.config.ts",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:webpanel\"",
    "dev:server": "nodemon --watch 'src/server/**/*.ts' --exec ts-node src/server/index.ts",
    "dev:webpanel": "vite src/webpanel --config src/webpanel/vite.config.ts",
    "test": "jest --config jest.config.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.4",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
